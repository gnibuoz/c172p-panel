<?xml version="1.0"?>

<PropertyList>
	<comment>
		<![CDATA[
			*******************OUTPUT************************
			Output Cessna 172P instruments data to UDP port 5500.
			CLI option: --generic=socket,out,20,ip-address,5500,udp,c172p-panel
		]]>
	</comment>

	<generic>
		<output>
			<line_separator>newline</line_separator>
			<var_separator>,</var_separator>

			<chunk>
				<name>vsi-fpm</name>
				<type>float</type>
				<format>vsi-fpm=%f</format>
				<node>/instrumentation/vertical-speed-indicator/indicated-speed-fpm</node>
			</chunk>
			<chunk>
				<name>asi-kt</name>
				<type>float</type>
				<format>asi-kt=%f</format>
				<node>/instrumentation/airspeed-indicator/indicated-speed-kt</node>
			</chunk>
			<chunk>
				<name>alt-ft</name>
				<type>float</type>
				<format>alt-ft=%f</format>
				<node>/instrumentation/altimeter/indicated-altitude-ft</node>
			</chunk>
			<chunk>
				<name>alt-inhg</name>
				<type>float</type>
				<format>alt-inhg=%f</format>
				<node>/instrumentation/altimeter/setting-inhg</node>
			</chunk>
			<chunk>
				<name>ai-pitch</name>
