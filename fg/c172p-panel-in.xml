<?xml version="1.0"?>

<PropertyList>
	<comment>
		<![CDATA[
			*****************INPUT*******************************
			Input Cessna 172P instruments data to UDP port 5501.
			CLI option: --generic=socket,in,30,ip-address,5501,udp,c172p-panel
		]]>
	</comment>

	<generic>
		<input>
			<line_separator>newline</line_separator>
			<var_separator>,</var_separator>

			<chunk>
				<name>alt-inhg</name>
				<type>float</type>
				<node>/instrumentation/altimeter/setting-inhg</node>
			</chunk>
			<chunk>
				<name>ai-offset</name>
				<type>float</type>
				<node>/instrumentation/attitude-indicator/horizon-offset-deg</node>
			</chunk>
			<chunk>
				<name>hi-bug-offset-deg</name>
				<type>float</type>
				<node>/autopilot/settings/heading-bug-deg</node>
			</chunk>
			<chunk>
				<name>hi-offset-deg</name>
				<type>float</type>
				<node>/instrumentation/heading-indicator/offset-deg</node>
			</chunk>
			<chunk>
				<name>nav-radial-selected-deg</name>
				<type>float</type>
				<node>/instrumentation/nav/radials/selected-deg</node>
			</chunk>
			<chunk>
				<name>1nav-radial-selected-deg</name>
				<type>float</type>
				<node>/instrumentation/nav[1]/radials/selected-deg</node>
			</chunk>
			<chunk>
				<name>comm-power-button</name>
				<type>bool</type>
				<node>/instrumentation/comm/power-btn</node>
			</chunk>
			<chunk>
				<name>comm-volume</name>
				<type>double</type>
				<node>/instrumentation/comm/volume</node>
			</chunk>
			<chunk>
				<name>comm-selected-mhz</name>
				<type>double</type>
				<node>/instrumentation/comm/frequencies/selected-mhz</node>
			</chunk>
			<chunk>
				<name>comm-standby-mhz</name>
				<type>double</type>
				<node>/instrumentation/comm/frequencies/standby-mhz</node>
			</chunk>
			<chunk>
				<name>1comm-power-button</name>
				<type>bool</type>
				<node>/instrumentation/comm[1]/power-btn</node>
			</chunk>
			<chunk>
				<name>1comm-volume</name>
				<type>double</type>
				<node>/instrumentation/comm[1]/volume</node>
			</chunk>
			<chunk>
				<name>1comm-selected-mhz</name>
				<type>double</type>
				<node>/instrumentation/comm[1]/frequencies/selected-mhz</node>
			</chunk>
			<chunk>
				<name>1comm-standby-mhz</name>
				<type>double</type>
				<node>/instrumentation/comm[1]/frequencies/standby-mhz</node>
			</chunk>
			<chunk>
				<name>nav-volume</name>
				<type>double</type>
				<node>/instrumentation/nav/volume</node>
			</chunk>
			<chunk>
				<name>nav-selected-mhz</name>
				<type>double</type>
				<node>/instrumentation/nav/frequencies/selected-mhz</node>
			</chunk>
			<chunk>
				<name>nav-standby-mhz</name>
				<type>double</type>
				<node>/instrumentation/nav/frequencies/standby-mhz</node>
			</chunk>
			<chunk>
				<name>1nav-volume</name>
				<type>double</type>
				<node>/instrumentation/nav[1]/volume</node>
			</chunk>
			<chunk>
				<name>1nav-selected-mhz</name>
				<type>double</type>
				<node>/instrumentation/nav[1]/frequencies/selected-mhz</node>
			</chunk>
			<chunk>
				<name>1nav-standby-mhz</name>
				<type>double</type>
				<node>/instrumentation/nav[1]/frequencies/standby-mhz</node>
			</chunk>
			<chunk>
				<name>dme-power-button</name>
				<type>bool</type>
				<node>/instrumentation/dme/power-btn</node>
			</chunk>
			<chunk>
				<name>dme-switch-min-kts</name>
				<type>bool</type>
				<node>/instrumentation/dme/switch-min-kts</node>
			</chunk>
			<chunk>
				<name>dme-switch-position</name>
				<type>int</type>
				<node>/instrumentation/dme/switch-position</node>
			</chunk>
			<chunk>
				<name>dme-source</name>
				<type>string</type>
				<node>/instrumentation/dme/frequencies/source</node>
			</chunk>
		</input>
	</generic>
</PropertyList>
